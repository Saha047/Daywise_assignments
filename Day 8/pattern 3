#include <stdio.h>

int main() {
	int i,j,k,c=1; 
    
    for (int i = 1; i <= 4; i++) {
        
        for (int j = 1; j <= 4 - i; j++) {
            printf(" ");
        }
        
        for (int j = 1; j <= i; j++) {
            printf("%d", c);
         c++; 
        }
        printf("\n"); 
    }
    
    return 0;
}
